#include<stdio.h>
int main()
{
    int n,k,p,a,b,i;
    while(scanf("%d",&a)==1)
    {
        for(i=1;i<=a;i++)
        {
            scanf("%d%d%d",&n,&k,&p);
            b=k+p;
            while(b>n)
            {
                b=b-n;
            }
            printf("Case %d: %d\n",i,b);
        }
    }
    return 0;
}
